# Note: This file is managed by Puppet.

<% if @remote_jmx_port -%>
# Enable JMX connections on port <%= remote_jmx_port %>
SERVER_JVMFLAGS="-Dcom.sun.management.jmxremote.port=<%= remote_jmx_port %> -Dcom.sun.management.jmxremote.ssl=false -Dcom.sun.management.jmxremote.authenticate=false"
<% end -%>
